{
  "strategy": {
    "name": "Volume Price Breakout Strategy",
    "description": "Cryptocurrency trading strategy based on volume and price breakouts",
    "version": "1.0.0",
    "mode": "paper"
  },
  "trading": {
    "symbol": "BTC/USDT",
    "timeframe": "1m",
    "exchange": "binance",
    "sandbox": true,
    "api_key": "",
    "secret": ""
  },
